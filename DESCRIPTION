Package: webtrackR
Title: Preprocessing and Analyzing Web Tracking Data
Version: 0.3.2
Authors@R: c(
    person("David", "Schoch", email = "david@schochastics.net", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0003-2952-4812")),
